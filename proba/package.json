{
  "name": "proba",
  "displayName": "Proba",
  "description": "Proba...",
  "author": "Qpx",
	"license": "MIT",
  "version": "0.0.1",
  "publisher": "qpx",
  "repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-extension-samples"
  },
  "keywords": [
		"multi-root ready"
	],
  "engines": {
    "vscode": "^1.45.0"
  },
  "categories": [
    "Programming Languages",
    "Themes",
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.proba",
    "onLanguage:plaintext"
  ],
  "main": "./client/out/extension",
  "contributes": {
    "themes": [
      {
        "label": "Proba Light",
        "uiTheme": "vs",
        "path": "./Proba_Light.tmTheme"
      },
      {
        "label": "Proba Dark",
        "uiTheme": "vs-dark",
        "path": "./Proba_Dark.tmTheme"
      }
    ],
    "languages": [
      {
        "id": "javascript",
        "extensions": [
          ".js"
        ],
        "aliases": [
          "js",
          "JavaScript"
        ],
        "configuration": "./js-configuration.json"
      }
    ],
    "commands": [
      {
        "command": "extension.proba",
        "title": "Proba :-)"
      }
    ],
    "configuration": {
			"type": "object",
			"title": "Example configuration",
			"properties": {
				"langServer.maxProblems": {
					"scope": "resource",
					"type": "number",
					"default": 100,
					"description": "Max problems by server."
				},
				"langServer.trace.server": {
					"scope": "window",
					"type": "string",
					"enum": [
						"off",
						"messages",
						"verbose"
					],
					"default": "off",
					"description": "Communication between VS Code and server."
				}
			}
		}
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -b",
    "lint": "eslint -p ./",
    "watch": "tsc -b -w",
    "pretest": "yarn run compile",
    "postinstall": "cd client && yarn install && cd ../server && yarn install && cd ..",
		"test": "sh ./scripts/e2e.sh"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@types/command-line-args": "",
    "@types/fs-extra": "",
    "@types/glob": "",
    "@types/mocha": "",
    "@types/vscode": "",
    "fs-extra": "",
    "glob": "",
    "mocha": "",
    "source-map-support": "",
    "vsce": "",
    "vscode-jsonrpc": "",
    "vscode-test": "",
    "vscode-uri": ""
  }
}
